<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hp.expensemanager">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dem"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewTripActivity" />
        <activity android:name=".ViewTrip2Activity" />
        <activity android:name=".DeleteUpdateActivity" />
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".BalanceActivity" />
        <activity android:name=".ViewTripWiseActivity" />
        <activity android:name=".ViewExpenseDetailsActivity" />
        <activity android:name=".ReportViewctivity" />
        <activity android:name=".ViewCategoryWiseActivity" />
        <activity android:name=".ViewDateWiseActivity"></activity>
    </application>

</manifest>
